{
  "name": "vue-component-weather-chipiona",
  "version": "1.0.0",
  "private": false,
  "main": "dist/v-component-weather-chipiona.cjs.js",
  "module": "dist/v-component-weather-chipiona.esm.js",
  "unpkg": "dist/v-component-weather-chipiona.global.js",
  "files": [
    "dist/",
    "src/"
  ],
  "browser": {
    "./sfc": "src/components/ChipionaWeatherComponent.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build_app": "vue-cli-service build",
    "build": "rollup -c build/rollup.config.js",
    "build1": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/v-component-weather-chipiona.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/v-component-weather-chipiona.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/v-component-weather-chipiona.min.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "rollup": "^2.52.7",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5"
  }
}
